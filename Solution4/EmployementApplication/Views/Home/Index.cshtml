<ng-view></ng-view>

    <script type="text/ng-template" id="myModalContent.html">
        <div ng-controller="EmployeeController">
            <div ng-init="getAllDept()">
                <form name="CreateEmp">
                    <div class="modal-header">
                        <h3 class="modal-title">Create</h3>
                    </div>
                    <div class="modal-body">
                        Name:<input required name="Name" type="text" ng-model="emp.Name" />
                        <span class="error" ng-show="CreateEmp.Name.$error.required" style="font-style:italic;color:grey">
                            enter a name
                        </span>
                        <br />
                        <br />
                        Email:<input required type="email" name="Email" ng-model="emp.Email" />
                        <span class="error"  ng-show="CreateEmp.Email.$error.required" style="font-style:italic;color:grey">
                            enter a valid Email
                        </span>
                       
                        <br />
                        <br />
                        Password:<input ng-pattern="/^[A-Z a-z 0-9]{6}[A-Z a-z 0-9]*$/" required type="password" name="Password" ng-model="emp.Password" />
                        <span class="error"  ng-show="CreateEmp.Password.$error.pattern" style="font-style:italic;color:grey">
                            enter a password with minimum 6 characters
                        </span>
                        <span class="error" ng-show="CreateEmp.Password.$error.required" style="font-style:italic;color:grey">
                            enter a password with minimum  6 characters
                        </span>
                        <br />
                        <br />
                        ConfirmPassword:<input type="password" name="ConfirmPassword" ng-model="emp.ConfirmPassword" />
                        <span class="error" ng-if="emp.Password!=emp.ConfirmPassword" style="font-style:italic;color:grey">
                            enter a matching password
                        </span>
                        <br />
                        <br />
                        Select Departments:<div ng-repeat="dept in departments">
                            <input type="checkbox" value="{{dept.Id}}" ng-checked="emp.DeptsValue.indexOf(dept.Id) > -1" ng-click="toggleSelection(dept.Id)" />{{dept.Name}}
                        </div>
                        <br/>
                        <div ng-show="loadingElement">         
                            <img src="data:image/gif;base64," width="54" height="54" />
                         
                        Please wait email confirmation link is being send...
                        </div>
                          </div>


                    <div class="modal-footer">
                        <button class="btn btn-primary" ng-click="saveEmp(emp,CreateEmp)">Create</button>
                        <button class="btn btn-warning" ng-click="cancel()">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
</script>



    <script type="text/ng-template" id="CreateDept.html">
       
        <div  ng-init="getAllDept()">
            <form name="CreateDept">
                <div class="modal-header">
                    <h3 class="modal-title">Create</h3>
                </div>
                <div class="modal-body">

                    Name:<input  required ng-trim="true" name="Name" type="text" ng-model="Dept.Name" />
                    <span class="error" ng-show="CreateDept.Name.$error.required" style="font-style:italic;color:grey">
                        enter a name
                    </span>
                    <div>
                        @*{{CreateDept.$invalid}}
                        {{CreateDept.$valid}}
                        {{CreateDept.$dirty}}
                        {{CreateDept.$error.required}}*@
                    </div>
                </div>
                
               
                <div class="modal-footer">
                    <button class="btn btn-primary" ng-trim="true"  ng-click="create(Dept,CreateDept)">Create</button>
                    <button class="btn btn-warning" ng-click="cancel()" >Cancel</button>
                </div>
             </form>
        </div>
  
    </script>


<script type="text/ng-template" id="EditDept.html">

    <div ng-init="getCurrDept()">
        <form name="EditDept">
            <div class="modal-header">
                <h3 class="modal-title">Edit</h3>
            </div>
            <div class="modal-body">

                Name:<input required ng-trim="true" name="Name" type="text" ng-model="currDeptData.Name" />
                <span class="error" ng-show="EditDept.Name.$error.required" style="font-style:italic;color:grey">
                    enter a name
                </span>
                <div>
                    @*{{CreateDept.$invalid}}
                        {{CreateDept.$valid}}
                        {{CreateDept.$dirty}}
                        {{CreateDept.$error.required}}*@
                </div>
            </div>


            <div class="modal-footer">
                <button class="btn btn-primary" ng-trim="true" ng-click="editDept(currDeptData,EditDept)">Edit</button>
                <button class="btn btn-warning" ng-click="cancel()">Cancel</button>
            </div>
        </form>
    </div>

</script>


<script type="text/ng-template" id="EditEmp.html">
   <div ng-controller="EmployeeController">
       <div ng-init="getEmp()">
           <form name="EditEmp">
               <div class="modal-header">
                   <h3 class="modal-title">Edit</h3>
               </div>
               <div class="modal-body">
                   Name:<input required name="Name" type="text" ng-model="emp.Name" />
                   <span class="error" ng-show="CreateEmp.Name.$error.required" style="font-style:italic;color:grey">
                       enter a name
                   </span>
                   <br />
                   <br />
                   Current Department:<br />
                   <div ng-repeat="dep in emp.DeptName"><li>{{dep}}</li></div>
                   <div ng-init="getAllDept()">
                       Select Departments:<div ng-repeat="dept in departments">
                           <input type="checkbox" value="{{dept.Id}}" ng-checked="emp.DeptsValue.indexOf(dept.Id) > -1" ng-click="toggleSelection(dept.Id)" />{{dept.Name}}
                       </div>
                   </div>
               </div>
               <div class="modal-footer">
                   <button class="btn btn-primary" ng-click="adminEmpEdit(emp,EditEmp)">Edit</button>
                   <button class="btn btn-warning" ng-click="cancel()">Cancel</button>
               </div>
           </form>
       </div>
    </div>
   
</script>